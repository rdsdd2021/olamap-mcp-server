{
  "name": "olamap-mcp-server",
  "version": "1.0.7",
  "description": "A Model Context Protocol server for OlaMap API integration with advanced trip planning",
  "main": "dist/index.js",
  "type": "module",
  "preferGlobal": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "olamap",
    "maps",
    "geocoding",
    "places",
    "routing",
    "elevation",
    "india",
    "trip-planning",
    "route-optimization",
    "ai-assistant",
    "location-intelligence"
  ],
  "author": "MCP Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdsdd2021/olamap-mcp-server.git"
  },
  "homepage": "https://github.com/rdsdd2021/olamap-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/rdsdd2021/olamap-mcp-server/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "bin": {
    "olamap-mcp-server": "dist/index.js",
    "mcp-server-olamap": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
